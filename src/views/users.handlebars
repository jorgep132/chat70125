<div>
    <h1>Bienvenido {{user.full_name}}</h1>
    {{!-- Como aplicar un if --}}
    {{#if isAdmin}}
    {{!-- for --}}
        {{#each users}}
        <div>
            <p>Nombre: {{this.full_name}} - Email: {{this.email}}</p>
            <hr>
        </div>
        {{/each}}
    {{else}}
        <h2>No tenes permisos para ver los usuarios</h2>
    {{/if}}
</div>
{{!-- Agregamos el script --}}
<script src="/static/assets/js/index.js"></script>